    <script>

        

        //// -- grafico uso
        var ctx = document.getElementById("ChartUSO");

        var nombresalCC = document.getElementById("nombresprof").value;
        var nombresal = nombresalCC.slice(0, -1);
        var promalCC = document.getElementById("horasprof").value;
        var promal = promalCC.slice(0, -1);

        var rString = nombresal.split(",");
        var rString2 = promal.split(",");
        var arralumnos = [];
        var arrprom = [];

        for (i = 0; i < rString.length; i++) {
            arralumnos.push(rString[i]);
            arrprom.push(rString2[i]);
        }

        var myLineChart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: arralumnos,
                datasets: [{
                    label: "Horas de uso",
                    backgroundColor: "rgba(2,117,216,1)",
                    borderColor: "rgba(2,117,216,1)",
                    data: arrprom,
                }],
            },
            options: {
                scales: {
                    xAxes: [{
                        time: {
                            unit: 'month'
                        },
                        gridLines: {
                            display: false
                        },
                        ticks: {
                            maxTicksLimit: 6
                        }
                    }],
                    yAxes: [{
                        ticks: {
                            min: 0,
                            max: 10,
                            maxTicksLimit: 5
                        },
                        gridLines: {
                            display: true
                        }
                    }],
                },
                legend: {
                    display: false
                }
            }
        });

        // -- Bar Chart Example
        var ctx = document.getElementById("myBarChart");

        var nombresalCC = document.getElementById("nombresal").value;
        var nombresal = nombresalCC.slice(0, -1);
        var promalCC = document.getElementById("promal").value;
        var promal = promalCC.slice(0, -1);

        var rString = nombresal.split(",");
        var rString2 = promal.split(",");
        var arralumnos = [];
        var arrprom = [];


        for (i = 0; i < rString.length; i++) {
            arralumnos.push(rString[i]);
            arrprom.push(rString2[i]);
        }

        var myLineChart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: arralumnos,
                datasets: [{
                    label: "Promedio Actual",
                    backgroundColor: "rgba(2,117,216,1)",
                    borderColor: "rgba(2,117,216,1)",
                    data: arrprom,
                }],
            },
            options: {
                scales: {
                    xAxes: [{
                        time: {
                            unit: 'month'
                        },
                        gridLines: {
                            display: false
                        },
                        ticks: {
                            maxTicksLimit: 6
                        }
                    }],
                    yAxes: [{
                        ticks: {
                            min: 0,
                            max: 70,
                            maxTicksLimit: 5
                        },
                        gridLines: {
                            display: true
                        }
                    }],
                },
                legend: {
                    display: false
                }
            }
        });
        
        // -- Pie Chart Example
        var ctx = document.getElementById("myPieChart");

        var nombresalCC = document.getElementById("promBMA").value;
        var nombresal = nombresalCC.slice(0, -1); 

        var rString = nombresal.split(","); 
        var arralumnos = []; 


        for (i = 0; i < rString.length; i++) {
            arralumnos.push(rString[i]); 
        }

        var myPieChart = new Chart(ctx, {
            type: 'pie',
            data: {
                labels: ["Peligro", "Normal", "Cuidado"],
                datasets: [{
                    data: arralumnos,
                    backgroundColor: ['#dc3545', '#28a745', '#ffc107'],
                }],
            },
        });
 
    </script>

